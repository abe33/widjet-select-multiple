{
  "name": "widjet-select-multiple",
  "version": "1.2.2",
  "description": "A widget to make multiple select more user friendly",
  "main": "lib/index.js",
  "jsnext:main": "src/index.es6",
  "scripts": {
    "test": "nyc mocha --timeout 5000 --recursive test/**/*-test.es6 test/*-test.es6",
    "lcov_report": "nyc report --reporter=lcov",
    "coverage": "npm run lcov_report && codecov",
    "babel": "babel src --out-dir lib",
    "lint": "eslint src/*.es6 test/*.es6",
    "lint:fix": "eslint --fix src/*.es6 test/*.es6",
    "esdoc": "esdoc -c esdoc.json",
    "prepublishOnly": "npm run babel",
    "start": "widjet-test-server test/**/*-test.es6"
  },
  "keywords": [
    "widgets",
    "widget",
    "web",
    "widjet",
    "select",
    "select multiple"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/abe33/widjet-select-multiple.git"
  },
  "bugs": "http://github.com/abe33/widjet-select-multiple/issues",
  "commits": "http://github.com/abe33/widjet-select-multiple/commits",
  "author": "Cédric Néhémie <cedric.nehemie@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.3",
    "@babel/register": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "babel-eslint": "^10.0.0",
    "codecov": "^3.8.3",
    "esdoc": "^1.1.0",
    "eslint": "^4.12.1",
    "eslint-config": "^0.3.0",
    "eslint-config-fbjs": "^1.1.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-flowtype": "^2.29.1",
    "eslint-plugin-jasmine": "^2.2.0",
    "eslint-plugin-prefer-object-spread": "^1.1.0",
    "eslint-plugin-react": "^5.2.2",
    "expect.js": "^0.3.1",
    "fbjs": "^0.8.6",
    "mocha": "^6.0.2",
    "mocha-jsdom": "^2.0.0",
    "nyc": "^13.3.0",
    "sinon": "^1.17.4",
    "jsdom": "^9.8.3",
    "widjet": "^1.1.0",
    "widjet-disposables": "^1.0.0",
    "widjet-utils": "^1.1.0",
    "widjet-test-utils": "^1.9.1"
  },
  "peerDependencies": {
    "widjet": "^1.1.0",
    "widjet-disposables": "^1.0.0",
    "widjet-utils": "^1.1.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "nyc": {
    "include": [
      "src/*.es6"
    ],
    "extension": [
      ".es6"
    ],
    "require": [
      "@babel/register"
    ]
  }
}
